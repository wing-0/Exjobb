@Article{Top2016,
  author        = {C. B. Top and A. K. Tafreshi and N. G. Gen√ßer},
  title         = {Microwave Sensing of Acoustically Induced Local Harmonic Motion: Experimental and Simulation Studies on Breast Tumor Detection},
  journal       = {IEEE Transactions on Microwave Theory and Techniques},
  year          = {2016},
  volume        = {64},
  number        = {11},
  pages         = {3974-3986},
  month         = {Nov},
  issn          = {0018-9480},
  __markedentry = {[tfy13nwi:2]},
  comment       = {The introduction of the article has much information on prior work in the field of acousto-electromagnetics with references. This is a good place to find more information.

The described method uses focused ultrasound, which is one of the techniques we have been discussing before. The way it works is that a focused ultrasound beam is amplitude modulated to create sinusoidally varying acoustic radiation force in its focus. The force causes harmonic motion in the part of the sample in under ultrasonic focus. This harmonic motion casues incoming microwaves to be Doppler shifted by the vibration frequency (which is twice the AM frequency). The Doppler component of scattered microwaves can then be measured. If the ultrasonic beam is scanned, the Doppler readings will give information on properties of the scanned material. The different properties contributing to the level of the Doppler component are primarily acoustic, elastic and electromagnetic properties. However, if the material is not completely homogeneous different scattering parts inside of the material complicates the picture. The scatterer size then becomes important as well. Vibration frequency also plays a part in the Doppler level.

Frequency of tissue vibration is twice the frequency of modulation in the ultrasonic beam. This is probably reasonable, but I need to be sure of why this is. After further reading it seems like they use different equations for this in different papers. Probably depends on how you define and generate your AM signal.

Throughout the derivations it is assumed that reflections of ultrasound are small. This might not be the case for laminated composite materials since the interface between layers might be a cause of reflections (if the difference in acoustic properties is large enough). Something to think about if this method is considered.

One issue with this method when compared with my work is that here the ultrasonic probe is scanned and the microwave equipment is static. My work relates to a mm-wave imaging system with a scanning mm-wave probe. To scan both a mm-wave probe and ultrasonic probe is totally unfeasible since the scan time would increase quadratically.},
  doi           = {10.1109/TMTT.2016.2607713},
  file          = {:Top2016.pdf:PDF},
  groups        = {Acousto-electromagnetics, Medical microwave frequencies, Localized harmonic motion},
  keywords      = {biomedical ultrasonics;Doppler measurement;finite difference time-domain analysis;mammography;microwave imaging;numerical analysis;phantoms;tumours;microwave sensing;acoustically induced local harmonic motion;breast tumor detection;microwave transceiver system;nonpalpable tumors;harmonic motion microwave Doppler imaging method;phase modulated signal;dielectric properties;elastic properties;acoustic properties;phantom materials;electrical properties;fibroglandular region size;vibration frequency;numerical simulations;tumor phantom inclusion;ultrasound probe;spectrum analyzer;finite-difference time-domain method;imaging system design;Phantoms;Microwave oscillators;Microwave theory and techniques;Microwave imaging;Vibrations;Tumors;Breast tumor detection;elasticity imaging;focused ultrasound (FUS);harmonic motion imaging;microwave imaging},
}

@Article{Ammari2012,
  author        = {Ammari, H. and Bossy, E. and Garnier, J. and Seppecher, L.},
  title         = {Acousto-electromagnetic Tomography},
  journal       = {SIAM Journal on Applied Mathematics},
  year          = {2012},
  volume        = {72},
  number        = {5},
  pages         = {1592-1617},
  __markedentry = {[tfy13nwi:1]},
  comment       = {It's clear that this paper was written by mathematicians. Lots of formalism and lengthy derivations. Lots of it seems to be related to the tomography part, which is not that relevant.

The method is based around imaging permittivity in a sample. It seems like they use two measurements: one with only electromagnetism and one with both acoustics and electromagnetism. The second measurement is where the acoustic waves alter the distribution of permittivity inside of the sample. Based upon these measurements it is somehow possible to recover the unperturbed permittivity distribution with a higher resolution than a standard electromagnetic-only measurement would give.

I can't find much information on interaction mechanisms in the paper, probably because they don't really rely on measuring the acousto-electromagnetic interaction directly (although this might be the case, but only hidden behind a massive amount of notation). The main mechanism is based on acoustic waves altering the permittivity, but I can't find anything more clear than that. While other papers usually look directly at a frequency shift caused by the interaction, this paper has a less direct method.

I'm very unsure if the methods described here are even applicable to our problem. Need to read this paper more times to be even close to answering that.},
  doi           = {10.1137/120863654},
  eprint        = {https://doi.org/10.1137/120863654},
  file          = {:Ammari2012.pdf:PDF},
  groups        = {Medical microwave frequencies},
}

@Article{Konofagou2003,
  author        = {Elisa E. Konofagou and Kullervo Hynynen},
  title         = {Localized harmonic motion imaging: theory, simulations and experiments},
  journal       = {Ultrasound in Medicine \& Biology},
  year          = {2003},
  volume        = {29},
  number        = {10},
  pages         = {1405 - 1413},
  issn          = {0301-5629},
  __markedentry = {[tfy13nwi:1]},
  comment       = {The article does not have any electromagnetic part, but is interesting anyway due to the ultrasonic techniques described. In the introduction the method of USAE (ultrasonic stimulated acoustic emission) is introduced. Here two ultrasound beams are used with slightly different frequencies f1 and f2. The two beams overlap in one spot where the interference leads to amplitude modulation fd = f2 - f1. This leads to objects becoming acoustic sources which is used in the article, which in itself is then used. But when objects in the intersection have a fd movement, there should be a fd doppler shift of EM waves in that region, but nowhere else. This is what is used in Top2016, but I think this article describes the acoustics better.

