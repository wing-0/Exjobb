  file     = {:Buerkle2009.pdf:PDF},
  groups   = {Acousto-electromagnetics, Michigan RadLab},
  keywords = {acoustic applications, electromagnetic wave scattering, nondestructive testing, non-destructive testing, elastic targets, acousto-electromagnetic wave interaction, reversal focusing, electromagnetic wave scattering, acoustic illumination, acoustic vibration, first Doppler component, Electromagnetic scattering, Acoustic waves, Acoustic scattering, Electromagnetic fields, Scattering parameters, Frequency modulation, Lighting, Vibrations, Predictive models, Bandwidth, Acoustic applications, electromagnetic scattering, focusing, nondestructive testing},
}

