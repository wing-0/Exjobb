@Article{Gurvich1987,
  author        = {Gurvich, A. S. and Kon, A. I. and Tatarskii, V. I.},
  title         = {Scattering of electromagnetic waves on sound in connection with problems of atmospheric sounding (review)},
  journal       = {Radiophysics and Quantum Electronics},
  year          = {1987},
  volume        = {30},
  number        = {4},
  pages         = {347--366},
