@Article{Rapoport1997,
  author        = {Rapoport, V. O. and Mityakov, N. A. and Zinichev, V. A. and Belova, N. I.},
  title         = {The role of atmospheric temperature gradients and winds for estimating the energy potential of radio-acoustic sounding systems},
  journal       = {Radiophysics and Quantum Electronics},
  year          = {1997},
  volume        = {40},
  number        = {5},
  pages         = {408--413},
  month         = {May},
  issn          = {1573-9120},
  __markedentry = {[tfy13nwi:1]},
  abstract      = {We present the theory of radio-acoustic sounding (RAS) of the atmosphere allowing for the dependence on temperature, altitude, and wind. For the case of a linear temperature profile, we obtained expressions for the received signal power. Since the acoustic wave front differs from the sphere if the temperature gradient is allowed for, we can introduce the notion of projector (Fresnel) and Fraunhofer regions. In the Fresnel region, the size of the diffraction spot on the earth is determined by radar antenna dimensions and the received signal power is proportional to z‚àí2. In the Fraunhofer region, the spot size is greater than the antenna dimensions and the power is proportional to z‚àí6. The existing RAS facilities work in the projector region. This can be used as a basis for developing a new method of diagnostics of a large-scale inhomogeneous atmospheric structure, including wave disturbances.},
  comment       = {Contains a derivation similar to Gurvich1987 but also considers wind. Wind is not of any concern to me, but at least this paper was scanned better than Gurvich1987 and the formulas are not half-invisible.

The very start of the derivation has a definition of permittivity which is only true in air. It is that the relative permittivity is "epsilon = 1 + epsilon1" where epsilon1 is a small perturbation. Air has relative permittivity ~1, but for other values of relative permittivity (let's call it er) the relation should be "epsilon = er + epsilon1". I need to analyze what would happen to the rest of the results if a change like this is introduced.

There is a pretty nice equation for the received power (analogous to the radar equation), although it contains a bunch of stuff related to wind. If I read the paper well enough I should be able to understand which variables are important and which are negligible. The size of both the acoustic and EM antennas seem to be important (relating to the beamwidth obviously). In the "radar equation" there is an exponential with a bunch of rho's and a vind velocity integral. I'm fairly sure this entire exponential is equal to one without wind and in a homogeneous material. If that is the case the equation is even more like a radar equation.},
  day           = {01},
  doi           = {10.1007/BF02676205},
  file          = {:Rapoport1997.pdf:PDF},
  groups        = {Radio acoustic sounding system},
}

@Book{Korpel1988,
